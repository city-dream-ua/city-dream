{
   "compilerOptions": {
      "experimentalDecorators": true,
      "target": "es2016",
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "useUnknownInCatchVariables": false,
      "skipLibCheck": true,
   }
}
